{
    "name": "stepanenko3/nova-boolean-group-field",
    "description": "Nova BooleanGroup advanced field based on native Nova field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^12.0.1",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaBooleanGroup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaBooleanGroup\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "tightenco/duster": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravelsatis.com"
        }
    ]
}
